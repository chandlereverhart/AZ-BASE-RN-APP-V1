{
  "expo": {
    "name": "AZ-BASE-App",
    "slug": "AZ-BASE-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/AppIcons/appstore.png",
    "splash": {
      "image": "src/assets/AZBASE-LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chandlereverhart.azbase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/AppIcons/playstore.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
