{
  "expo": {
    "name": "AZ-BASE-App",
    "slug": "AZ-BASE-App",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "src/assets/AppIcons/appstore.png",
    "splash": {
      "image": "src/assets/AZBASE-LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.chandlereverhart.azbase",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow this app to use your location."
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/AppIcons/playstore.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.chandlereverhart.azbase"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
